#include "standard-types.hsl"
struct sVector3
{
float x;
float y;
float z;
}sVector3;

struct sPATH_POINT
{
	sVector3	_Pos;
	float		_PrevDist;
}sPATH_POINT;

struct sPATH_LIST
{
DWORD tblidx;
	float						_fRadius;
	BYTE						_NumPathPoint;
	sPATH_POINT	_vecPathPoint[_NumPathPoint];
}sPATH_LIST;

struct sPATH_TABLE
{

	unsigned int	_NumPathList;
	sPATH_LIST			_mapPathList;
};